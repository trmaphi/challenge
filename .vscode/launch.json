    "version": "0.2.0",
    "configurations": [     
        {
            "name": "Launch Golang File",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Launch Node Server",
            "program": "${workspaceFolder}/part3/server.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "envFile": "${workspaceFolder}/part3/.env",
        }
    ]
}